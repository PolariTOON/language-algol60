{
  "name": "language-algol60",
  "version": "0.5.3",
  "displayName": "Algol 60 syntax highlighter",
  "description": "Algol 60 language support in Atom, Pulsar and Visual Studio Code",
  "icon": "./logotypes/language-algol60-200x200.png",
  "keywords": [
    "language",
    "grammar"
  ],
  "categories": [
    "Programming Languages",
    "Snippets"
  ],
  "repository": "https://github.com/PolariTOON/language-algol60",
  "author": "PolariTOON",
  "publisher": "TrisTOON",
  "license": "MIT",
  "engines": {
    "atom": "^1.0.0",
    "pulsar": "^1.0.0",
    "vscode": "^1.44.0"
  },
  "contributes": {
    "languages": [
      {
        "id": "algol60",
        "extensions": [
          ".a60",
          ".alg",
          ".algol",
          ".algol60"
        ],
        "configuration": "./settings/language-algol60.json"
      }
    ],
    "grammars": [
      {
        "language": "algol60",
        "scopeName": "source.algol60",
        "path": "./grammars/language-algol60.json"
      }
    ],
    "snippets": [
      {
        "language": "algol60",
        "path": "./snippets/language-algol60.json"
      }
    ]
  }
}
